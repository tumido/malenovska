{
  "name": "malenovska-react",
  "version": "1.1.0",
  "description": "Malenovska.cz in React-Redux based on react-redux-boilerplate",
  "repository": {
    "type": "git",
    "url": "git://github.com/tumido/malenovska-react.git"
  },
  "homepage": "https://tumido.github.io/malenovska-react",
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "author": "Tomas Coufal",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "clean": "npm run test:clean && npm run build:clean",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "eslint:fix": "eslint -- . --ignore-path .gitignore --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "predeploy": "npm run build",
    "deploy": "firebase deploy --only hosting",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "deploy_no_build": "firebase deploy --only hosting",
    "gen": "generact",
    "export": "firestore-export -a malenovska-service-account.json -p"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "chalk": "^2.4.2",
    "compression": "1.7.4",
    "cross-env": "^5.2.0",
    "express": "4.17.1",
    "firebase": "^6.3.4",
    "fontfaceobserver": "2.1.0",
    "hoist-non-react-statics": "3.3.0",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "leaflet": "^1.5.1",
    "lodash": "^4.17.15",
    "minimist": "1.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.3",
    "react-burger-menu": "^2.6.11",
    "react-dom": "^16.8.3",
    "react-helmet": "5.2.1",
    "react-leaflet": "^2.4.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.1.0",
    "react-redux": "7.1.0",
    "react-redux-firebase": "^3.0.0-alpha.14",
    "react-router-dom": "^5.0.1",
    "redux": "4.0.4",
    "redux-burger-menu": "^0.2.8",
    "redux-firestore": "^0.8.0",
    "redux-form": "^8.2.5",
    "redux-saga": "1.0.5",
    "sanitize.css": "11.0.0",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-react-constant-elements": "^7.5.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "circular-dependency-plugin": "5.0.2",
    "css-loader": "3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-redux-saga": "^1.0.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "file-loader": "4.1.0",
    "firebase-tools": "^7.2.2",
    "generact": "^0.4.0",
    "gh-pages": "^2.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "^5.0.0",
    "imports-loader": "0.8.0",
    "jest-cli": "^24.8.0",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-firestore-import-export": "^0.14.0",
    "node-plop": "^0.19.0",
    "node-sass": "^4.12.0",
    "null-loader": "3.0.0",
    "plop": "2.4.0",
    "react-test-renderer": "^16.8.3",
    "rimraf": "2.6.3",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "style-loader": "^1.0.0",
    "url-loader": "2.1.0",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
