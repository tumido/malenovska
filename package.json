{
  "name": "malenovska-react",
  "version": "2.0.1",
  "description": "Malenovska.cz in React-Redux based on react-redux-boilerplate",
  "repository": {
    "type": "git",
    "url": "git://github.com/tumido/malenovska-react.git"
  },
  "homepage": "https://tumido.github.io/malenovska-react",
  "author": "Tomas Coufal",
  "license": "MIT",
  "scripts": {
    "build": "webpack -p --config config/prod.webpack.config.js --progress --profile --json > .bundle-stats.json",
    "analyze": "webpack-bundle-analyzer .bundle-stats.json dist",
    "start": "webpack-dev-server --config config/dev.webpack.config.js --progress --color",
    "prod": "NODE_ENV=production webpack-dev-server --config config/prod.webpack.config.js --progress",
    "lint": "eslint --ignore-path .gitignore",
    "predeploy": "npm run build",
    "deploy": "firebase deploy --only hosting",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "export": "firestore-export -a malenovska-service-account.json -p"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.2",
    "@material-ui/lab": "^4.0.0-alpha.42",
    "@material-ui/styles": "^4.9.0",
    "@material-ui/system": "^4.9.1",
    "clsx": "^1.1.0",
    "core-js": "^3.6.4",
    "firebase": "^7.8.1",
    "leaflet": "^1.6.0",
    "markdown-to-jsx": "^6.11.0",
    "notistack": "^0.9.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "5.2.1",
    "react-leaflet": "^2.6.1",
    "react-redux": "7.1.3",
    "react-redux-firebase": "^3.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-firestore": "^0.12.0",
    "redux-form": "^8.3.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "smoothscroll-polyfill": "^0.4.4",
    "webfontloader": "^1.6.28",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime-corejs3": "^7.8.4",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "5.0.2",
    "firebase-tools": "^7.13.0",
    "html-webpack-plugin": "3.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-firestore-import-export": "^0.14.1",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
