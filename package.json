{
  "name": "malenovska-react",
  "version": "2.0.0",
  "description": "Malenovska.cz in React-Redux based on react-redux-boilerplate",
  "repository": {
    "type": "git",
    "url": "git://github.com/tumido/malenovska-react.git"
  },
  "homepage": "https://tumido.github.io/malenovska-react",
  "author": "Tomas Coufal",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js --progress",
    "start": "webpack-dev-server --config config/dev.webpack.config.js --progress --color",
    "prod": "NODE_ENV=production webpack-dev-server --config config/prod.webpack.config.js",
    "lint": "eslint --ignore-path .gitignore",
    "predeploy": "npm run build",
    "deploy": "firebase deploy --only hosting",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "export": "firestore-export -a malenovska-service-account.json -p"
  },
  "dependencies": {
    "firebase": "^6.3.4",
    "leaflet": "^1.5.1",
    "prop-types": "^15.6.2",
    "react": "^16.8.3",
    "react-burger-menu": "^2.6.11",
    "react-dom": "^16.8.3",
    "react-helmet": "5.2.1",
    "react-leaflet": "^2.4.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.1.0",
    "react-redux": "7.1.0",
    "react-redux-firebase": "^3.0.0-alpha.14",
    "react-router-dom": "^5.0.1",
    "redux": "4.0.4",
    "redux-burger-menu": "^0.2.8",
    "redux-firestore": "^0.8.0",
    "redux-form": "^8.2.6",
    "redux-logger": "^3.0.6",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "4.1.0",
    "firebase-tools": "^7.2.2",
    "html-webpack-plugin": "3.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.8.0",
    "node-firestore-import-export": "^0.14.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  }
}
